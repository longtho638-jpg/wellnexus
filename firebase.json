{
  "functions": {
    "source": "functions",
    "runtime": "nodejs18"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/api/onboarding/**",
        "function": "onboarding"
      },
      {
        "source": "/api/affiliate/**",
        "function": "affiliate"
      },
      {
        "source": "/api/ad/**",
        "function": "ad"
      },
      {
        "source": "/api/evidence/**",
        "function": "evidence"
      }
    ]
  }
}
